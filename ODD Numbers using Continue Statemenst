
public class OddNum {
	// print the Odd numbers from 1 to 100 .
	
	    public static void main(String[] args) {
	    	
//		 Odd --> 1 , 3 ,5 , 7 ,9 , .....,99
	    	
	    	   for (int i =0 ; i <=100 ; i++) {
	    		   if (i%2==0) {
	    			   continue ;
	    		  }
	   		     System.out.println(i);
	      }
	   }
	}


