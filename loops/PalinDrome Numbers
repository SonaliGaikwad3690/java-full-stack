// Palindrome Numbers
public class PalindromeNum {
	
	public static void main(String[] args) {
		
//		int  i=3443, -->3443//palindrome number reverse same as input
		
		int num = 4334;
		int temp=num;
		int rev=0;
		int rem=0;
		while(num>0) {
			rem=num%10;
			rev=(rev*10)+rem;
			num=num/10;
		}
		if(rev==temp){
			System.out.println("Palindrome Number");
		}
			else {
			System.out.println(" no");
	  }
	}
}

