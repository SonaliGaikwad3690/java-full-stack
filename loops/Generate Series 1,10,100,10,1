// Generate the Series 1 ,10 ,100,1000
public class GenerateSeries1 {

	public static void main(String[] args) {
		          int total=1;
				 for(int i=1;i<=4;i++) {
					 System.out.print(total + " ");
					 total=total*10;
				 }
				   total=total/100;
				 for(int i=1;i<=4;i++) {
					 System.out.print(total + " ");
					 total=total/10;
					
			 }
       	}
	}
	


