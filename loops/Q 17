// Print the Reverse number example :- 112233
public class Q17 {

	public static void main(String[] args) {
		
		int num=112233;
		int rem=0;
		int rev=0;
		int count =0;
		while(num>=1) {
			count++;
			rem=num%10;
			rev=rev*10+rem;
			num=num/10;
		}num++;
		System.out.println("The Reverse number = " + rev);
		System.out.println("The Count of Reverse number is = " + count);
		
	}
}
