// Print the Reverse Number 238956
public class Q29 {

	public static void main(String[] args) {
		
		int num=238956;
		int rev=0;
		int rem=0;
		int count =0;
//		while(num>0) {
		for(int i=1;i<=num;) {
			rem=num%10;
			rev=rev*10+rem;
			num=num/10;
			count++;
		}System.out.println("The Reverse Numebr = " + rev);
		System.out.println("Count the Digits In Given Number = " + count);
	}
}
