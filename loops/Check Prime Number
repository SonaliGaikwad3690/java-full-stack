package com.tka;

public class CheckPrimeNum {

	public static void main(String[] args) {

//		 check the number is prime or Not number =25;
//		prime number have a only 2 Factors that one is 1 and Itself

		int num = 25;
		int count = 0;
		for (int i = 1; i <= 25; i++) { // Number will check till i become 25
			if (num % i == 0) {
				count++;
			}
		}
		if (count == 2) {
			System.err.println(num + " Number is a Prime number...!");
		} else {
			System.out.println(num + " Number is not a Prime number....!");

		}
	}

}
