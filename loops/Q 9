// Check The Given Year is Leap or Not
public class Q9 {

	public static void main(String[] args) {
		
		int year=2002;
		if ((year% 400==0) || (year%4==0 && year%100!=0)) {
			
			 System.out.println( " This is Leap Year..!");
		} else {
		    	System.out.println(" This is not Leap Year..!");
		  }
		}
	}

