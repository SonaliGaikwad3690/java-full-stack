// Month Seasons
public class Q8 {
	public static void main(String[] args) {
		
		int month=1;
		switch(month) {
		case 11:
		case 12:
		case 1 :
		case 2 :
			System.out.println("Winter Season...!");
//			break;
		case 3 :
	    case 4 :
	    case 5 :
		case 6 :
			System.out.println("Summer Season ...!");
//			break;
		case 7 :
		case 8 :
		case 9 :
		case 10 :
			System.out.println("Rainy Season ...!");
			break;
			default:
				System.out.println(" Invalid");
		}
	}

}
