
public class Hcfnum2 {
	
	public static void main(String[] args) {
		
		int num1 =22;
		int num2= 18;
		int hcf=1;
		int div=2;
		
		while(div<=num1 && div<=num2) {
			
		if(num1%div==0 && num2%div==0 ) {
			num1=num1/div;
			num2=num2/div;
			hcf=hcf*div;
			
		}   else {
			   div++;	
		    }
		  }  System.out.println( "HCF of " + num1 + " And " + num2 + " = "  + hcf);    
 	}
}

