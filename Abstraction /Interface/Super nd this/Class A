package com.package1;

public class A {

	int a = 20 ;
	public A (){ // Constructor
		System.out.println("This is a Constructor....!");
	}
	
	public A (int a) {
		this();
		System.out.println("This is 2nd Constuctor...!");
	}
	
	public void m1() { // Method 
		System.out.println("This is a m1 method....!");
	}
}
