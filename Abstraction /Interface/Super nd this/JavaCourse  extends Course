package com.SuperAndThis;

public class JavaCourse  extends Course{
      int hoursPerModule;
	JavaCourse(String courseName ,int totalModule , int hoursPerModule) {
		//this Course ();
		super(courseName, totalModule);
		this.hoursPerModule = hoursPerModule ;
	}

	@Override
	public void calculateDuration() {
		int totalHours = totalModule * hoursPerModule ;
		super.courseDetails();
		System.out.println("Java Course Duration = " + totalHours + " hours");
	}

	
}
