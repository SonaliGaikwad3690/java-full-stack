package com.SuperAndThis;

public  class Student {
    String name;
	int id ;
	
	Student (){
		//this("Unknown" , 22);
		System.out.println("This is Default Contr Called....!");
	}
	
	Student (String name , int id){
	this();
		this.name = name ;
		this.id = id ;
		System.out.println("Parameterised Constr Called....!");
	}
}
