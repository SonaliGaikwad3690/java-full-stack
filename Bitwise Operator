
public class OperatorDay5 {
	
	public static void main(String [] args ) {
       
		System.out.println(17 & 44);  //0
		System.out.println(17 & 55);  // 17 
		System.out.println(17 | 55);  // 55
		System.out.println(8 >> 3);   //1
		System.out.println(13 << 3);   //104
		System.out.println(13 >> 5);    //0
  }
}
