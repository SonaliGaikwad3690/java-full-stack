package com.String;
//Print Each Character in Separate Line
//Input: "Core" , Output:  C , o , r , e
public class SeparateLineChar {

	public static void main(String[] args) {
		
		String str = "Core";
		for(int i = 0;i<str.length(); i++) {
			char ch = str.charAt(i);
			System.out.println(ch);
		}
	}
}
