package com.PracticeBaseQue;

public class PalindromeNum {
	public static void main(String[] args) {
		int num = 3443;
        int org = num ;
        int rem= 0;
        int rev = 0;
        while(num>0) {
        	rem = num%10;
        	rev = rev *10 + rem;
        	num = num/10;
        }
        if(org==rev) {
        	System.out.println(rev + " is a Palindrom Number...!");
        }
        else {
        	System.out.println(rev + " is Not a Palindrome Number...!");
        }
	}
}
