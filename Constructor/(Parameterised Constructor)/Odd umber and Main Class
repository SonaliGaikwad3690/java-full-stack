package com.ParameterisedConstructor;

public class OddNumber {
    int a ;
	OddNumber(int a){
		this.a = a;
	}
	public void display () {
		for(int i =1;i<=10;i++) {
			if(i%2==0) {
			System.out.println(i);
			}
		}
	}
	public static void main(String[] args) {
		OddNumber num = new OddNumber(10);
		num.display();
	}
}
