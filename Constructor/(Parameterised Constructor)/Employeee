package com.ParameterisedConstructor;

public class Employeee {

	int empId ;
	String name;
	Employeee(int empId , String name){
		this.empId = empId;
		this.name = name;
	}
	 
	void display () {
		System.out.println("Employee Id = " + empId);
		System.out.println("Employee name = " + name);
		System.err.println("------");
	}
	
}
