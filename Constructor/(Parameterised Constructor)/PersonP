package com.ParameterisedConstructor;
// Write a program for copy Constructor 
public class PersonP {

	String name;
	int age;

	PersonP(String name, int age) {
		this.name = name;
		this.age = age;
	}

	PersonP(PersonP p) { // copy Constructor
		this.name = p.name;
		this.age = p.age ;
	}
    public void display() {
    	System.out.println("Person Name = " + name);
     	System.out.println("Person Age = " + age);
     	System.err.println("-------");
    }
}
