
public class StarPattern5 {

     public static void main(String[] args) {
		
		 for (int i=1 ; i<=5; i++) { // for rows
//			inner loop for space print
			  for (int j=1; j<=5-i; j++) { // for column 
				 System.out.print("   "); //  printing the space for column
	     	 } 
			  for (int j=1; j<=i; j++) { // this loop using for printing the star *
				  System.out.print(" * ");
		    }
			  System.out.println("  ");   //  Space For Next line 
	   }
	}
}


