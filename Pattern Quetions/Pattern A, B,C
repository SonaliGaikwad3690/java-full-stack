
public class Pattern3 {

	public static void main(String[] args) {
		for(int i=1;i<=3;i++) {
			int value = 65;
			for(int  j=1;j<=3;j++) {
				System.out.print( (char)value + " ");
				value++;
			}System.out.println(" ");
		}
		
		for(char ch ='A';ch<='Z'; ch++) {
			for(ch='A'; ch<='Z'; ch++) {
			System.out.print(ch + " ");
			
		  }System.out.println(" ");
		}
	}
}
