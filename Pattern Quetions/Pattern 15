
public class Pattern15 {

	public static void main(String[] args) {
		
		for(int i=1;i<=3;i++) {
			
	     	for(int j=1;j<=3-i;j++) { // this for loop for print the space
				
				System.out.print(" ");
			}
				 for(int k=1;k<=i;k++) { //This For loop for Print the stars
					System.out.print("* ");
				} System.out.println(" ");
			}
	}
}
	    
		

