
public class CheckPrime {
// Check the Number is Prime or Not
	public static void main(String[] args) {

		int num = 30;
		int count = 0;
		if (num <= 1) {
			System.out.println(num + " is not a Prime numebr...!");
		} else {
			for (int i = 1; i <= num; i++) {
				if (num % i == 0) {
					count++;
				}
			}
			if (count == 2) {
				System.out.println(num + "is a Prime Number...!");
			} else {
				System.err.println(num + " is Not a Prime Numebr");
			}
		}
	}
}
