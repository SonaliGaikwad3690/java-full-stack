//		   i = 4 ;                  //4
//		  System.out.println(i--);  //3
//		   i = 5+2 ;                //7
//		   i++ ;                    //8
//		  System.out.println(i--);  //7
//           i++ ;                    //8
//          System.out.println(i);    // ANS 8
   }
} 
