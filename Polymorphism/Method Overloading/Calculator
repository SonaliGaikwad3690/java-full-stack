package com.Polymorphism;

public class Calculator {

	public void add(int a, int b) {
		System.out.println("Addition = " + (a + b));
	}
	
	public void add(int a, long b) {
		System.out.println("Addition = " + (a + b));
	}
	
	public void add(int a, int b, int c) {
		System.out.println("Addition = " + (a + b + c));
	}
	
}
