
public class RelationalOperatorTask {
	
    public static void main(String[] args) {
	
//    	Relational Operator Task... <,>,==,<=,>=,!=
    	int a = 10;
    	int b = 20;
    	System.out.println(a <= b);
    	System.out.println(a > b);
    	System.out.println(a != b);
    	System.out.println(a == b);
    	System.out.println(a >= b);
    	System.out.println(a < b);
   }
}
