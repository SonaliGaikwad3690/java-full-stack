// print the Even numbers from 1 to 100 .
 public class EvenNumber {
    public static void main(String[] args) {
//	 even num --> 0 , 2 , 4 , 6 ,8 , .....,100
    	
    	   for (int i =0 ; i <=100 ; i++) {
    		   if (i%2==1) {
    			   continue ;
    		  }
   		     System.out.println(i);
      }
   }
}
