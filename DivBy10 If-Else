
public class DivBy10 {
	public static void main(String[] args) {
		
//		Write a program to check whether a number is divisible by 10.
		int num = 25;
		if(num%10==0) {
			System.out.println("This Number is Divisible By 10 = " + num);
		}	else
			System.out.println("This Number is Not Divisible By 10..!");
	}
}
