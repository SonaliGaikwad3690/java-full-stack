
public class StarPattern4 {
	
	public static void main(String[] args) {
		
		for(int i =1; i<=5; i++) { // for Rows
			for(int j =1;j<=5; j++) {  //for Column
				if (i==1 || j==1 || i==5 || j==5) {
				System.out.print("* ");
			} else {
				System.out.print("  ");
			}
			
		}
			System.out.println(""); // for Space
	  }
   }
}
